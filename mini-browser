git init
git add .
git commit -m "Initial commit"
git branch -M main
git remote add origin https://github.com/yourusername/mini-browser.git
git push -u origin main
cd path/to/your/mini-browser
mini-browser/
├─ main.py
├─ requirements.txt
├─ templates/
│  └─ index.html
├─ static/ (optional for CSS/JS)
pip freeze > requirements.txt
if __name__ == "__main__":
    app.run(host="0.0.0.0", port=8080)
